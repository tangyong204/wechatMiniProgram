/**index.wxss**/

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.swiper {
  width: 100%;
}
.cate-section {
  display: flex;
  flex-flow: row;
  justify-items: center;
  width: 100%;
  height: 200rpx;
  margin-top: 20rpx;
}
.cate-section .cate-item{
  width: 20%;
  height: 100%;
  padding-left: 2%;
}
.cate-section .cate-item image{
  width: 120rpx;
  height: 120rpx;
}
.panel{
  width: 100%;
  margin-bottom: 40rpx;
}
.content{
  padding: 20rpx;
}